          "_name": "tabs2",
          "_sourceTheme": "medicalm4",
          "_customHTML": "<section class=\"tabs2 tabs medicalm4_tabs2\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" group=\"Tabs\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\" plugins=\"mbr-tabs\">\n\n    <mbr-parameters>\n        <input inline max=\"9\" min=\"0\" name=\"paddingTop\" step=\"1\" title=\"Top\" type=\"range\" value=\"4\">\n        <input inline max=\"9\" min=\"0\" name=\"paddingBottom\" step=\"1\" title=\"Bottom\" type=\"range\" value=\"4\">\n        <header>Show / Hide</header>\n        <input name=\"showTitle\" title=\"Title\" type=\"checkbox\" checked>\n        <input name=\"showWidget\" title=\"Widget\" type=\"checkbox\">\n        <input name=\"showTabImage\" title=\"Tab Image\" type=\"checkbox\">\n        <input checked name=\"showTabTitle\" title=\"Tab Title\" type=\"checkbox\">\n        <input checked name=\"showTabText\" title=\"Tab Text\" type=\"checkbox\">\n        <input name=\"showTabCards\" title=\"Tab Cards\" type=\"checkbox\">\n        <input checked condition=\"showTabCards\" name=\"swIcon\" title=\"Card Icons\" type=\"checkbox\">\n        <input checked condition=\"showTabCards\" name=\"showCardTitle\" title=\"Card Title\" type=\"checkbox\">\n        <input checked condition=\"showTabCards\" name=\"showCardText\" title=\"Card Text\" type=\"checkbox\">\n        <header>Colors</header>\n        <input name=\"tabsColor\" title=\"Tabs\" type=\"color\" value=\"#28303f\">\n        <input name=\"activeTabColor\" title=\"Active Tab\" type=\"color\" value=\"#ffffff\">\n        <input name=\"activeTabbg\" title=\"Active Tab BG\" type=\"color\" value=\"#4f87fb\">\n        <input condition=\"showTabCards\" name=\"cardBg\" title=\"Card Bg\" type=\"color\" value=\"#ffffff\">\n        <input condition=\"showWidget\" name=\"widgetBg\" title=\"Widget Bg\" type=\"color\" value=\"#4f87fb\">\n        <select name=\"bootstrapTabs\" title=\"Tabs Count\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\">8</option>\n            <option selected value=\"9\">9</option>\n        </select>\n\n        <select condition=\"showTabCards\" name=\"cardsAmount\" title=\"Cards amount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option selected value=\"3\">3</option>\n            <option value=\"4\">4</option>\n        </select>\n        <header>Background</header>\n        <fieldset name=\"bg\" parallax type=\"background\">\n            <input parallax title=\"Image\" type=\"image\" value=\"../_images/background1.jpg\">\n            <input selected title=\"Color\" type=\"color\" value=\"#ffffff\">\n            <input title=\"Video\" type=\"video\" value=\"http://www.youtube.com/watch?v=uNCr7NdOJgw\">\n        </fieldset>\n        <input checked condition=\"bg.type !== 'color'\" name=\"overlay\" title=\"Overlay\" type=\"checkbox\">\n        <input condition=\"overlay&&bg.type !== 'color'\" name=\"overlayColor\" title=\"Overlay Color\" type=\"color\" value=\"#ffffff\">\n        <input condition=\"overlay&&bg.type !== 'color'\" inline max=\"1\" min=\"0\" name=\"overlayOpacity\" step=\"0.1\" title=\"Opacity\" type=\"range\" value=\"0.7\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\"></div>\n    <div class=\"container\">\n        <h2 class=\"align-left mbr-bold pb-3 mbr-fonts-style\" mbr-if=\"showTitle\" mbr-theme-style=\"display-2\">Meriter: Erfarenhet och utbildning</h2>\n        <div class=\"row\">\n            <div class=\"col-md-4\">\n                <ul class=\"nav nav-tabs\" role=\"tablist\">\n                    <li class=\"nav-item firs